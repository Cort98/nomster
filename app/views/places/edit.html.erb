<div class="booyah-box col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-3">
  <h1>Edit details below</h1>
  <br />
  <%= simple_form_for @place do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.submit 'Save', class: 'btn btn-primary' %>
  <% end %>
  
</div>
